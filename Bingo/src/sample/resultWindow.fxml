<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ResultWindow">
    <children>
        <Button fx:id="againButton" layoutX="125.0" layoutY="110.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: lightgreen;" text="Ещё раз">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="125.0" layoutY="148.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: pink;" text="Выйти">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Text fx:id="textResult" layoutX="25.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="(ТЕКСТ С РЕЗУЛЬТАТОМ)" textAlignment="CENTER" wrappingWidth="350.0">
            <font>
                <Font size="18.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
